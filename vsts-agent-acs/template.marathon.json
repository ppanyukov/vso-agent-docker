{
  "id": "vsts-agent",
  "cmd": null,
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "instances": 0,
  "container": {
    "docker": {
      "parameters": [
        {
          "key": "stop-signal",
          "value": "SIGQUIT"
        }
      ],
      "network": "HOST",
      "image": "%IMAGE_TAG%"
    },
    "type": "DOCKER",
    "volumes": []
  },
  "portDefinitions": [
    {
      "port": 0,
      "protocol": "tcp",
      "name": null,
      "labels": null
    }
  ],
  "env": {
    "VSTS_AGENT_NAME_PREFIX": "your_prefix",
    "VSTS_AUTH_TYPE": "PAT",
    "VSTS_AUTH_TOKEN": "your_token",
    "VSTS_POOL": "your_agent_pool_name",
    "VSTS_URL": "https://your_company.visualstudio.com"
  },
  "labels": {},
  "healthChecks": []
}
